  "title": "PC-Style Shortcuts customized",
  "rules": [
    {
        "description": "Change Kor/Eng key to command+space customized",
        "manipulators": [
        {
          "from": {
            "key_code": "lang1"
          },
          "to": [
            {
              "key_code": "spacebar",
              "modifiers": [
                "left_control"
            ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
        "description": "Change Hanja key to option+return customized",
        "manipulators": [
        {
          "from": {
            "key_code": "lang2"
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": [
                "right_alt"
            ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "^org\\.gnu\\.Emacs$"
              ]
            }
          ],
          "type": "basic"
        }
      ]
